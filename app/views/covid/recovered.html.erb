<h1>Recovered Cases:</h1>

<table>
<thead>
  <td><b>Country:</b></td>
  <td><b>State:</b></td>
  <td><b>Recovered: </b></td>
  <td><b> Updated_at:</b></td>
</thead>

<tbody>
  <% @recovered.each do |recovered| %>
    <tr>
      <td><%= recovered['countryRegion']%></td>
      <td><%= recovered['provinceState']%></td>
      <td><%= recovered['confirmed']%></td>
      <td><%= Time.at(recovered['lastUpdate'].to_i/1000).strftime('%d/%m/%Y %H:%M:%S')%></td>
    </tr>

  <% end %>
  <button onclick="goBack()">Go Back</button>
  <script>
  function goBack() {
    window.history.back()
  }
  </script>
</tbody>
</table>
